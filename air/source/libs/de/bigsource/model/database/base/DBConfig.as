package de.bigsource.model.database.base {	import flash.utils.getDefinitionByName;		/**	 * @author alexanderfunke	 */	public class DBConfig 	{		//Configure Path to Tables		public static var tablepackage : String = "";		//Get the Table		public static function getTable(p_name : String) : Object		{			var tmpobj : Object = getDefinitionByName(tablepackage+"."+p_name) as Object;			return tmpobj.clone();		}	}}